Depends: macathena-config-common
CompileScript: <<
<<
InstallScript: <<
	mkdir -p %d/private/etc
	cp ssh_config.macathena %d/private/etc
<<
PostInstScript: <<
	source %p/share/macathena-config-common/divert.sh
	package=%n
	
	divert_link /private/etc/ssh_config
<<
